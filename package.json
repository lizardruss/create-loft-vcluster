{
  "name": "create-vcluster",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub Action to create a Loft VCluster",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "npm run package:main && npm run package:post",
    "package:main": "ncc build lib/main.js --source-map --license licenses.txt --out dist/main",
    "package:post": "ncc build lib/post.js --source-map --license licenses.txt --out dist/post",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loft-sh/create-vcluster.git"
  },
  "keywords": [
    "actions",
    "loft",
    "vcluster"
  ],
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.0",
    "@actions/io": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^16.3.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vercel/ncc": "^0.29.0",
    "eslint": "^7.31.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^27.1.1",
    "jest": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.2.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.1.3"
  }
}